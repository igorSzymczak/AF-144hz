package extensions {
  public class RibbonHistoryEntry {
      public var x0:Number;
      public var y0:Number;
      public var x1:Number;
      public var y1:Number;
      public var alpha:Number;
      public var time:Number;

      public function RibbonHistoryEntry(x0:Number, y0:Number, x1:Number, y1:Number, alpha:Number, time:Number) {
          this.x0 = x0;
          this.y0 = y0;
          this.x1 = x1;
          this.y1 = y1;
          this.alpha = alpha;
          this.time = time;
      }
  }
}

